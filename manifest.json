{
	"manifest_version": 2,
	"name": "YouTube Bubble Breaker",
	"description": "Break free from YouTube suggestion algorithm bubble",
	"homepage_url": "http://localhost",
	"version": "1.0",
	"icons": {
		"48": "icons/icon.png"
	},
	"permissions": [
		"storage"
	],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["./index.js"]
		}
	]
}
