{
	"name": "youtube-bubble-breaker",
	"version": "1.0.0",
	"description": "",
	"main": "./dist/index.js",
	"author": "",
	"private": true,
	"scripts": {
		"dev": "npx nodemon -w ./src/ -e ts,js --exec npm run build",
		"build": "esbuild ./src/index.ts --bundle --sourcemap=inline --format=iife --outfile=\"./dist/index.js\" && npx cpx static/**/* dist/",
		"start": "npx web-ext run -f firefoxdeveloperedition -u https://youtube.com -s ./dist/"
	},
	"dependencies": {
		"esbuild": "0.16.10",
		"tslib": "2.4.1",
		"web-ext": "^7.4.0",
		"youtubei.js": "2.6.0"
	},
	"devDependencies": {
		"@types/chrome": "0.0.206",
		"@types/node": "16.9.0",
		"cpx": "^1.5.0",
		"nodemon": "2.0.20",
		"typescript": "4.7.4",
		"web-ext-types": "3.2.1"
	}
}
